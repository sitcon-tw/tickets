{
	"printWidth": 200,
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"singleQuote": false,
	"trailingComma": "none",
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "never",
	"plugins": ["prettier-plugin-organize-imports"],
	"overrides": []
}
